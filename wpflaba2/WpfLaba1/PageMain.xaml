        <TextBlock Margin="28,109,461,323"><Hyperlink><Run Text="Ценные бумаги"/></Hyperlink></TextBlock>
        <TextBlock Margin="28,146,508,284" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.289"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.162"/>
                </TransformGroup>
            </TextBlock.RenderTransform><Hyperlink><Run Text="Сделки"/></Hyperlink></TextBlock>
        <TextBlock Height="19" Margin="28,164,500,267"><Hyperlink><Run Text="Справка"/></Hyperlink></TextBlock>
        <Button Style="{StaticResource BackgroundButtonResource}" 
